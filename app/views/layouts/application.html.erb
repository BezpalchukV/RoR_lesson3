<!DOCTYPE html>
<html>
<head>
  <title>RoRLesson3</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= link_to 'Main page', root_path %>
<%= link_to 'Registration', registration_path unless current_user %>
<%= link_to 'Login', sessions_login_path unless current_user %>
<%= link_to 'Logout', sessions_logout_path if current_user %>

<%= current_user.name if current_user%>

<%= yield %>

</body>
</html>
